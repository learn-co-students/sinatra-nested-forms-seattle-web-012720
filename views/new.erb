<h1>Pirate Form </h1>
<form action="/pirates" method="post">
  Pirate Name: <input type="text" name="pirate[name]">
  Pirate Weight: <input type="number" name="pirate[weight]">
  Pirate Height: <input type="number" name="pirate[height]"><br>
  Ship 1 Name: <input id="ship_name_1"type="text" name="pirate[ships][][name]">
  Ship 1 Type: <input id="ship_type_1"type="text" name="pirate[ships][][type]">
  Ship 1 Booty: <input id="ship_booty_1"type="text" name="pirate[ships][][booty]"><br>
  Ship 2 Name: <input id="ship_name_2"type="text" name="pirate[ships][][name]">
  Ship 2 Type: <input id="ship_type_2"type="text" name="pirate[ships][][type]">
  Ship 2 Booty: <input id="ship_booty_2"type="text" name="pirate[ships][][booty]"><br>
  <br>
  <input id="Submit" type="submit">
</form>